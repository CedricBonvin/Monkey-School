ethods : {\n    },\n    mounted(){\n        // AFFICHE LE MENU BURGER\n            const menu = document.getElementById(\"topbarMenu\")\n            const burger = document.getElementById(\"burger\")\n            menu.setAttribute(\"data-displaymenu\", \"false\")\n\n        document.body.addEventListener(\"click\", function(e){\n            if (e.target === burger && menu.dataset.displaymenu === \"false\"){\n                menu.classList.remove(\"menuSlideOut\")\n                menu.classList.add(\"menuSlideIn\")\n                menu.dataset.displaymenu = \"true\"\n            }else if ( menu.dataset.displaymenu === \"true\"){\n                menu.classList.remove(\"menuSlideIn\")\n                menu.classList.add(\"menuSlideOut\")\n                menu.dataset.displaymenu = \"false\"\n            }\n        })\n    }\n}\n</script>\n\n\n\n<style scoped>\n   .test{\n       overflow: hidden;\n   }\n    .fa-bars{\n        font-size: 2em;\n        cursor: pointer;\n    }\n    ul{\n        position: absolute;\n        padding: 20px;\n        height: 100vh;\n        z-index: 3;\n        right: -125px;\n        top: 50px;\n        background-color: rgb(32, 32, 32);\n        list-style-type: none;\n        opacity: 0;\n    }\n    .menuSlideIn{\n        animation: slideIN 1s forwards;\n    }\n    .menuSlideOut{\n        animation: menuSlideOut 1s forwards;\n    }\n    li{\n        padding: 15px 0;\n        margin: 0 10px;\n    }\n \n    a.router-link-exact-active {\n        color: #42b983;\n        font-size: 1.2rem;\n        font-weight: bold;\n    }\n    a {\n        color: white;\n        text-decoration: none;\n        transition: .3s;\n    }\n    a:hover{\n        display:inline-block;\n        transform: scale(1.2);\n        font-weight: bold;\n        text-decoration: underline;\n        \n    }\n    #burger{\n        font-size: 1.7em;  \n        margin: 0;  \n        padding: 0;   \n    }\n \n</style>"]}]}